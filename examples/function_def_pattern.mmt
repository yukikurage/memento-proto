// examples/function_def_pattern.mmt

// Define a simple ADT for optional numbers
data SomeNum : (value : number) => NumBool;

type NumBool := SomeNum;

val extract_num : (nb : NumBool) => number 
  := (nb : NumBool) => 
    switch(nb) [
      (SomeNum(n) : NumBool) => n
    ];

val main : (input : number) => number 
  := (tmp : number) => extract_num(SomeNum(42));