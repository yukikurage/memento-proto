// Multi-parameter polymorphic types
data Pair<A, B> : (first : A, second : B) => Pair;

// Multi-parameter polymorphic functions  
val fst<A, B> : (p : Pair, a : A, b : B) => A := (p : Pair, a : A, b : B) => a;
val swap<A, B> : (first : A, second : B) => Pair := (first : A, second : B) => Pair(second, first);

// Usage
val my_pair : Pair := Pair(42, true);
val swapped : Pair := swap(100, false);